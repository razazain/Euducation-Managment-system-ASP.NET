@{
    ViewData["Title"] = "Add Exam type";
}
@model ZealEducationSystem.Models.ExamType
<div class="content-body">

    <div class="row page-titles mx-0">
        <div class="col p-md-0">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Dashboard</a></li>
                <li class="breadcrumb-item active"><a asp-controller="Home" asp-action="Add_Exam_Type">Add Exam Type</a></li>
            </ol>
        </div>
    </div>
    <!-- row -->

    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <h1>ADD EXAM TYPE</h1>
                <div class="card">
                    <div class="card-body">
                        <div class="form-validation">
                            <form class="form-valide" method="post">

                                <div class="form-group row">
                                    <label class="col-lg-4 col-form-label" for="val-username">
                                        Exam Type <span class="text-danger">*</span>
                                    </label>
                                    <div class="col-lg-6">
                                        <input asp-for="ExamType1" type="text" class="form-control" placeholder="Write Exam Type">
                                    </div>
                                </div>




                                <div class="form-group row">
                                    <div class="col-lg-8 ml-auto">
                                        <input value="Submit" type="submit" class="btn btn-primary">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- #/ container -->
</div>

